<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>UserInfoServlet</servlet-name>
        <servlet-class>cn.happy.servlet.UserInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserInfoNewServlet</servlet-name>
        <servlet-class>cn.happy.servlet.UserInfoNewServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>UserInfoServlet</servlet-name>
        <url-pattern>/UserInfoServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>UserInfoNewServlet</servlet-name>
        <url-pattern>/UserInfoNewServlet</url-pattern>
    </servlet-mapping>
</web-app>
